{
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": ["error", { "properties": "never" }],
    "eqeqeq": ["error", "always"],
    "no-unused-vars": ["error"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
